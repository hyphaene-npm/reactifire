{
	"name": "{{name}}",
	"version": "0.0.0",
	"description": "{{description}}",
	"author": "{{author}}",
	"license": "{{license}}",
	"main": "build/index.cjs.js",
	"module": "build/index.esm.js",
	"browser": "build/index.js",
	"style": "build/index.css",
	"scripts": {
		"build": "NODE_ENV=dev rollup -c",
		"lint:js": "eslint --ext .js,.jsx .",
		"lint:css": "stylelint **/*.scss",
		"prepare": "npm run build",
		"start": "NODE_ENV=dev rollup -c -w",
		"storybook": "start-storybook -p 6006",
		"test": "NODE_ENV=test jest",
		"test:commit": "NODE_ENV=test jest --findRelatedTests",
		"test:cov": "NODE_ENV=test jest --coverage --passWithNoTests",
		"test:full-cov": "NODE_ENV=test jest --coverage",
		"precommit": "lint-staged",
		"patch": "versionifier patch",
		"minor": "versionifier minor",
		"major": "versionifier major"
	},
	"keywords": [
		"rollup",
		"react",
		"react component",
		"rollup react",
		"component"
	],
	"dependencies": {
		"object-assign": "^4.1.1",
		"promise": "^8.0.1",
		"prop-types": "{{propTypes}}",
		"react": "{{react}}",
		"react-dom": "{{react}}",
		"@storybook/react": "^5.2.8",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^5.2.8",
		"@storybook/addon-actions": "^5.2.8",
		"@storybook/addon-centered": "^5.2.8",
		"@storybook/addon-console": "^1.0.0",
		"@storybook/addon-info": "^5.2.8",
		"@storybook/addon-options": "^5.2.8",
		"@storybook/addons": "^5.2.8",
		"@storybook/channels": "^5.2.8",
		"@storybook/storybook-deployer": "^2.3.0",
		"autoprefixer": "^9.7.3",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^24.9.0",
		"babel-loader": "^8.0.6",
		"babel-preset-env": "^1.6.1",
		"babel-preset-minify": "^0.5.1",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^3.3.0",
		"enzyme": "^3.3.0",
		"eslint": "^6.7.2",
		"eslint-import-resolver-webpack": "^0.12.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-react": "^7.11.1",
		"history": "^4.7.2",
		"husky": "^3.1.0",
		"jest": "^24.9.0",
		"lint-staged": "^9.5.0",
		"node-sass": "^4.8.3",
		"postcss": "^7.0.24",
		"rollup": "^1.27.10",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-filesize": "^6.2.1",
		"rollup-plugin-import-alias": "^1.0.4",
		"rollup-plugin-local-resolve": "^1.0.7",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.1.0",
		"rollup-plugin-postcss": "^2.0.3",
		"sass-loader": "^8.0.0",
		"source-map-loader": "^0.2.3",
		"style-loader": "^1.0.1",
		"stylelint": "^12.0.0",
		"stylelint-scss": "^3.1.0",
		"stylelint-z-index-value-constraint": "^1.0.0",
		"versionifier": "^1.3.4",
		"webpack": "^4.5.0",
		"webpack-build-notifier": "^2.0.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.1.3"
	},
	"versionifier": {
		"commitIfOnlyPackageJsonInStage": true,
		"commitIfMultipleFilesInStage": true,
		"push": true,
		"commitMessage": "Updated package.json to version @@",
		"remoteRepo": "origin",
		"publish": true
	}
}
